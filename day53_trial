import java.util.Scanner;

public class day53 {

    public static void main(String[] args) {
            Scanner scan=new Scanner(System.in);

            System.out.println("================================");
            for(int i=0;i<5;i++){
                String a =sc.next();
                int b =sc.nextInt();
                System.out.printf("out : " , a , b);
                
            }
            System.out.println("================================");

    }
}

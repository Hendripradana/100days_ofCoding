public class Day50 {
    public static void main(String[] args) {
        String [] nilai = new String[4];

        nilai[0] = "5";
        nilai[1] = "7";
        nilai[2] = "8";
        nilai[3] = "4";
        

        for(int i=0;i<nilai.length;i++){
            System.out.println("Index ke-"+i+" = "+nilai[i]);
        }
    }
}

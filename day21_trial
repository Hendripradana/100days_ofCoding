public class Day21 {
    public static void main(String[] args) {
        String [] nilai = new String[4];

        nilai[0] = "10";
        nilai[1] = "20";
        nilai[2] = "30";
        nilai[3] = "40";
        

        for(int i=0;i<nilai.length;i++){
            System.out.println("Index ke-"+i+" = "+nilai[i]);
        }
    }
}
